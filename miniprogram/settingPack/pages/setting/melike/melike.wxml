<my-topstatus bgcolor="white"></my-topstatus>
<view class="page">
     <view class="page__bd">
      <my-listwrap column="{{column}}" imgurllist="{{imgurllist}}" requestAction="{{requestAction}}" requesturl="{{requesturl}}" bind:callback="onReqBack"></my-listwrap>
      <my-loadmore  index = "{{loadmore}}" bind:retry="onRetry"></my-loadmore>
    </view>
</view>
<my-scrolltop scrollTopShow = "{{scrolltopshow}}" bind:scrollTopBack="onScrollTopBack"></my-scrolltop>